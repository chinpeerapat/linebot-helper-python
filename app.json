{
    "name": "linebot Information helper",
    "description": "LINEBo Information helper with Google Gemini",
    "repository": "https://github.com/kkdai/inebot-helper-python",
    "keywords": [
        "python",
        "linebot",
        "gemini",
        "firebase"
    ],
    "buildpacks": [
        {
            "url": "https://github.com/heroku/heroku-buildpack-python.git"
        },
        {
            "url": "heroku/python"
        }
    ],
    "env": {
        "GEMINI_API_KEY": {
            "description": "GEMINI API KEY",
            "required": true
        },
        "ChannelAccessToken": {
            "description": "Channel Access Token",
            "required": true
        },
        "ChannelSecret": {
            "description": "Channel Secret",
            "required": true
        }
    }
}